@model GameResult

<div class="mdl-card mdl-shadow--2dp">
    <div class="mdl-card__title">
        <h2 class="mdl-card__title-text">@Model.BrainGame.Name</h2>
    </div>
    <div class="mdl-card__supporting-text">
        <strong>@Html.DisplayNameFor(g => g.Score)</strong>
        @Html.DisplayFor(g => g.Score)
    </div>
    <div class="mdl-card__supporting-text">
        <strong>@Html.DisplayNameFor(g => g.DateTime)</strong>
        @Html.DisplayFor(g => g.DateTime)
    </div>
</div>